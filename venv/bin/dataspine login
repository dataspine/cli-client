#!/Users/asadkazmi/Work/blockx/dataspine/venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'DataSpine-Authenticate','console_scripts','dataspine login'
__requires__ = 'DataSpine-Authenticate'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('DataSpine-Authenticate', 'console_scripts', 'dataspine login')()
    )
